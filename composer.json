{
    "require": {
        "php": "^8.0.0",
        "vlucas/phpdotenv": "^5.4",
        "symfony/routing": "^6.1",
        "symfony/http-foundation": "^6.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "^8.3",
        "phpstan/phpstan": "^1.8",
        "symfony/var-dumper": "^6.1",
        "psy/psysh": "@stable"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    }
}
